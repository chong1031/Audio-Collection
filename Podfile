source 'https://github.com/CocoaPods/Specs.git'

platform :ios, '8.0'
xcodeproj 'ObenProto.xcodeproj'
link_with 'ObenProto'


use_frameworks!

target 'ObenProto' do
    pod 'SwiftHTTP', :git=> "https://github.com/daltoniam/SwiftHTTP.git", :branch=>"swift-2-bc"
    pod 'SwiftyJSON'
    pod 'Signals'
    pod 'Fabric'
    pod 'Crashlytics'
    pod 'SWTableViewCell'
    pod 'HanekeSwift'
end